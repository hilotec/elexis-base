<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description>Build Elexis Core with test features (ch.elexis.core.test.feature) and run JUnit tests.</description>
  <logRotator>
    <daysToKeep>-1</daysToKeep>
    <numToKeep>4</numToKeep>
    <artifactDaysToKeep>-1</artifactDaysToKeep>
    <artifactNumToKeep>-1</artifactNumToKeep>
  </logRotator>
  <keepDependencies>false</keepDependencies>
  <properties/>
  <scm class="hudson.plugins.mercurial.MercurialSCM">
    <source>https://bitbucket.org/ngiger/elexis-base</source>
    <modules></modules>
    <branch>2.1.buckminster</branch>
    <clean>false</clean>
    <forest>false</forest>
    <browser class="hudson.plugins.mercurial.browser.HgWeb">
      <url>https://bitbucket.org/ngiger/elexis-base/</url>
    </browser>
  </scm>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers class="vector">
    <hudson.triggers.SCMTrigger>
      <spec>*/15 * * * *</spec>
    </hudson.triggers.SCMTrigger>
  </triggers>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.plugins.buckminster.EclipseBuckminsterBuilder>
      <installationName>Buckminster</installationName>
      <commands>import &apos;${WORKSPACE}/ch.elexis.releng/buckminster/local.core.test.cquery&apos;
build  -c
emma -l &apos;ch.elexis_test/CoreAllTests.launch&apos; -o &apos;${WORKSPACE}/test-reports/CoreAllTests.xml&apos; --flatXML --xml &apos;${WORKSPACE}/test-coverage-reports/CoreAllTests.xml&apos;
emma -l &apos;ch.rgw.utility_test/UtilityAllTests.launch&apos; -o &apos;${WORKSPACE}/test-reports/UtilityAllTests.xml&apos; --flatXML --xml &apos;${WORKSPACE}/test-coverage-reports/UtilityAllTests.xml&apos;</commands>
      <logLevel>info</logLevel>
      <params></params>
      <targetPlatformName>Elexis Core Target</targetPlatformName>
      <userTemp></userTemp>
      <userOutput></userOutput>
      <userCommand></userCommand>
      <userWorkspace></userWorkspace>
      <globalPropertiesFile></globalPropertiesFile>
      <equinoxLauncherArgs></equinoxLauncherArgs>
    </hudson.plugins.buckminster.EclipseBuckminsterBuilder>
  </builders>
  <publishers>
    <hudson.tasks.junit.JUnitResultArchiver>
      <testResults>test-reports/*.xml</testResults>
      <keepLongStdio>false</keepLongStdio>
      <testDataPublishers/>
    </hudson.tasks.junit.JUnitResultArchiver>
    <hudson.plugins.emma.EmmaPublisher>
      <includes>test-coverage-reports/*.xml</includes>
      <healthReports>
        <minClass>0</minClass>
        <maxClass>100</maxClass>
        <minMethod>0</minMethod>
        <maxMethod>70</maxMethod>
        <minBlock>0</minBlock>
        <maxBlock>80</maxBlock>
        <minLine>0</minLine>
        <maxLine>80</maxLine>
      </healthReports>
    </hudson.plugins.emma.EmmaPublisher>
    <hudson.tasks.Mailer>
      <recipients></recipients>
      <dontNotifyEveryUnstableBuild>true</dontNotifyEveryUnstableBuild>
      <sendToIndividuals>true</sendToIndividuals>
    </hudson.tasks.Mailer>
  </publishers>
  <buildWrappers/>
</project>

<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="true"
            category="org.iatrix"
            class="org.iatrix.views.JournalView"
            fastViewWidthRatio="0.2"
            id="org.iatrix.views.JournalView"
            name="KG Iatrix"/>
      <category
            id="org.iatrix"
            name="Iatrix"/>
      <view
            category="org.iatrix"
            class="org.iatrix.views.ProblemView"
            id="org.iatrix.views.ProblemView"
            name="Problem"/>
      <view
            class="org.iatrix.views.KonsListView"
            id="org.iatrix.views.KonsListView"
            name="Test KonsListView">
      </view>
      <view
            allowMultiple="true"
            category="org.iatrix"
            class="org.iatrix.views.WebBrowserView"
            icon="icons/applications-internet.png"
            id="org.iatrix.views.WebBrowserView"
            name="%iatrix.WebBrowserView">
      </view>
   </extension>
   <extension
         point="ch.elexis.PersistentReference">
      <Factory
            Class="org.iatrix.data.ProblemFactory"
            name="Problem"/>
   </extension>
   <extension
         point="org.eclipse.ui.contexts">
      <context
            id="org.iatrix.view.context"
            name="%iatrix.view.context.name"
            parentId="org.eclipse.ui.contexts.window">
      </context>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="org.iatrix.commands.newcons"
            contextId="org.iatrix.view.context"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+K">
      </key>
      <key
            commandId="org.iatrix.commands.export_clipboard"
            contextId="org.iatrix.view.context"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+E">
      </key>
      <key
            commandId="org.iatrix.commands.newproblem"
            contextId="org.iatrix.view.context"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+P">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            categoryId="org.iatrix.commands.category"
            id="org.iatrix.commands.newcons"
            name="%iatrix.commands.newcons">
      </command>
      <category
            id="org.iatrix.commands.category"
            name="%iatrix.commands.category">
      </category>
      <command
            categoryId="org.iatrix.commands.category"
            id="org.iatrix.commands.export_clipboard"
            name="%iatrix.commands.export_clipboard">
      </command>
      <command
            categoryId="org.iatrix.commands.category"
            id="org.iatrix.commands.newproblem"
            name="%iatrix.commands.newproblem">
      </command>
   </extension>
</plugin>
